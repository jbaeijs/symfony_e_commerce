{# templates/Default/contact.html.twig #}
{% extends "base.html.twig" %}

{% block body %}
    <h1>{% trans %}Boutique.rayon.titre{% endtrans %}</h1>
    {% if produits is defined %}
        <div class="container">
            <div class="row">
                {% for produit in produits %}
                    <div class="col-sm">
                        <img src="{{ asset(produit.visuel) }}"/>
                        <h1>{{ produit.libelle }}<h1>
                    </div>
                {% endfor %}
            </div>
        </div>
    {% endif %}
{% endblock %}  