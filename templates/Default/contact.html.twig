{# templates/Default/contact.html.twig #}
{% extends "base.html.twig" %}

{% block body %}
    <h1>{% trans %} default.contact.title {% endtrans %}</h1>
    <form>
        <div class="form-group">
            <label for="nom">{% trans %}default.contact.nom{% endtrans %}</label>
            <input type="text" class="form-control" id="nom" placeholder="...">
        </div>
        <div class="form-group">
            <label for="prenom">{% trans %}default.contact.prenom{% endtrans %}</label>
            <input type="text" class="form-control" id="prenom" placeholder="...">
        </div>
    </form>        
{% endblock %}